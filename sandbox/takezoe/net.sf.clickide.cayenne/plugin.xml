         point="org.eclipse.ui.editors">
      <editor
            filenames="cayenne.xml"
            id="net.sf.clickide.cayenne.editor.CayenneModelerLauncher"
            launcher="net.sf.clickide.cayenne.editor.CayenneModelerLauncher"
            name="Cayenne Modeler">
