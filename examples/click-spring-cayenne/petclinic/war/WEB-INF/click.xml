<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<click-app>
    
  <pages package="org.springframework.samples.petclinic.page" automapping="true">
    <page path="home.htm" classname="BasePage"/>
  </pages>
  
  <headers>
    <header name="Pragma" value="no-cache"/>
    <header name="Cache-Control" value="no-store, no-cache, must-revalidate, post-check=0, pre-check=0"/>
    <header name="Expires" value="1" type="Date"/>
  </headers>
  
  <mode value="development"/>
  
</click-app>